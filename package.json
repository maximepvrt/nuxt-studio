{
  "name": "content-preview",
  "version": "0.0.1",
  "description": "Content Preview for Nuxt Content",
  "repository": "@nuxtlabs/content-preview",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "npm run dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@farnabaz/mdc-editor": "0.0.1",
    "@nuxt/kit": "^4.0.3",
    "@nuxt/ui": "^3.3.2",
    "@nuxt/ui-pro": "^3.3.2",
    "@tailwindcss/typography": "^0.5.16",
    "@unhead/vue": "^2.0.14",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vueuse/core": "^13.8.0",
    "debug": "^4.4.1",
    "eslint": "^9.34.0",
    "eslint-plugin-vue": "^10.4.0",
    "idb-keyval": "^6.2.2",
    "minimatch": "^10.0.3",
    "nuxt-auth-utils": "^0.5.23",
    "vue": "^3.5.20",
    "vue-router": "^4.5.1",
    "vue-tsc": "^3.0.6"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.6.3",
    "@nuxt/eslint-config": "^1.9.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.0.3",
    "@nuxt/test-utils": "^3.19.2",
    "@nuxt/ui-pro": "3.3.2",
    "@types/node": "latest",
    "changelogen": "^0.6.2",
    "eslint": "^9.33.0",
    "nuxt": "^4.0.3",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.41.0",
    "vite": "^7.1.3",
    "vite-plugin-libcss": "^1.1.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.5"
  },
  "resolutions": {
    "@nuxt/content": "https://pkg.pr.new/@nuxt/content@8488733"
  },
  "packageManager": "pnpm@10.15.0"
}
