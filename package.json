{
  "name": "nuxt-studio",
  "version": "0.0.1",
  "description": "Nuxt Studio for Nuxt Content",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt-content/studio.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./dist/module/module.mjs",
    "./app": {
      "types": "./dist/app/main.d.ts",
      "default": "./dist/app/main.js"
    },
    "./app/utils": {
      "types": "./dist/app/utils.d.ts",
      "default": "./dist/app/utils.js"
    },
    "./app/service-worker": {
      "types": "./dist/app/service-worker.d.ts",
      "default": "./dist/app/service-worker.js"
    }
  },
  "files": [
    "dist"
  ],
  "main": "./dist/module/module.mjs",
  "scripts": {
    "prepack": "nuxt-module-build build src/module; vite build src/app",
    "dev": "npm run dev:prepare && STUDIO_DEV_SERVER=http://localhost:5151 nuxi dev playground/docus",
    "dev:minimal": "npm run dev:prepare && STUDIO_DEV_SERVER=http://localhost:5151 nuxt dev playground/minimal",
    "dev:app": "vite src/app --port 5151",
    "dev:build": "nuxi build playground/docus",
    "dev:prepare": "nuxt-module-build build --stub src/module && nuxt-module-build prepare src/module && nuxi prepare playground/docus",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "typecheck": "nuxt typecheck",
    "verify": "npm run lint && npm run typecheck && npm run test",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground/docus && vue-tsc --noEmit"
  },
  "devDependencies": {
    "@nuxt/content": "https://pkg.pr.new/@nuxt/content@2d21c02",
    "@nuxt/eslint-config": "latest",
    "@nuxt/kit": "^4.1.3",
    "@nuxt/module-builder": "latest",
    "@nuxt/ui": "^4.0.1",
    "@octokit/types": "^15.0.0",
    "@tailwindcss/typography": "latest",
    "@types/js-yaml": "^4.0.9",
    "@vitejs/plugin-vue": "^6.0.1",
    "eslint": "^9.37.0",
    "modern-monaco": "^0.2.2",
    "nuxt-studio": "workspace:*",
    "vite": "^7.1.9",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-libcss": "^1.1.2",
    "vitest": "^3.2.4",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1",
    "vue-tsc": "^3.1.1",
    "zod": "^4.1.12"
  },
  "resolutions": {
    "@nuxt/content": "https://pkg.pr.new/@nuxt/content@2d21c02"
  },
  "packageManager": "pnpm@10.18.1",
  "dependencies": {
    "@iconify-json/simple-icons": "^1.2.54",
    "@nuxtjs/mdc": "0.18.0",
    "@nuxtlabs/monarch-mdc": "^0.8.1",
    "@unhead/vue": "^2.0.19",
    "@unpic/vue": "^1.0.0",
    "@vueuse/core": "^13.9.0",
    "defu": "^6.1.4",
    "destr": "^2.0.5",
    "idb-keyval": "^6.2.2",
    "js-yaml": "^4.1.0",
    "minimark": "^0.2.0",
    "ofetch": "^1.4.1",
    "unstorage": "^1.17.1"
  },
  "keywords": [
    "nuxt",
    "content",
    "nuxt-module",
    "studio",
    "editor",
    "preview",
    "cms",
    "markdown",
    "mdc"
  ]
}
